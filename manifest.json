{
  "manifest_version": 3,
  "version": "1",
  "name": "Miles Morales Neon Snowy Night Spider Man",
  "description": "Just One Click to Absolute Elegance!",

  "theme": {
    "colors": {
      "frame": [20, 21, 26],
      "toolbar": [61, 64, 76],
      "tab_text": [255, 255, 255],
      "tab_background_text": [255, 255, 255],
      "bookmark_text": [255, 255, 255],
      "ntp_background": [20, 21, 26]

    },      


  "tints" : {
      "buttons": [1, 1, 1],
      "frame_incognito": [ -1, -1, -1],
      "frame_inactive": [ -1, -1, -1],
      "frame_incognito_inactive": [  -1, -1, -1]

    },

    
    "images": {
      "theme_ntp_background": "miles-morales-neon-snowy-night-spider-man_1920x1080.png",
      "theme_ntp_attribution" : "images/attribution.png"
    },


    "properties": {
      "ntp_background_alignment":"bottom",
      "ntp_background_repeat":"repeat-x",
      "ntp_logo_alternate":1

    }

  }
}